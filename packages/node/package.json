{
  "name": "@styleguide/node",
  "description": "Node implementations for styleguide",
  "version": "0.0.0-alpha.0",
  "homepage": "https://github.com/gherrink/styleguide/",
  "keywords": [
    "styleguide",
    "ui-kit",
    "node"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/packages/node/src/index.d.ts",
      "import": "./dist/index.js",
