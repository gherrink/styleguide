    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "umd": "dist/index.umd.js",
  "types": "dist/packages/node/src/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gherrink/styleguide",
    "directory": "packages/node"
  },
  "dependencies": {
    "picomatch": "^4.0.2"
  },
  "devDependencies": {
    "@styleguide/core": "workspace:^0.0.0-alpha.0",
    "@types/node": "^20.12.7",
    "@types/picomatch": "^2.3.3"
  },
  "peerDependencies": {
    "@styleguide/core": "workspace:^0.0.0-alpha.0"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "$npm_execpath clean && rollup -c",
    "test": "jest"
  }
