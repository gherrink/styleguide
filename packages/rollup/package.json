{
  "name": "@styleguide/rollup",
  "description": "Rollup plugin for styleguide.",
  "version": "0.0.0-alpha.0",
  "license": "MIT",
  "keywords": [
    "styleguide",
    "ui-kit",
    "documentation",
    "generation",
    "rollup",
    "rollup-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gherrink/styleguide",
    "directory": "packages/rollup"
  },
  "author": "Maurice Busch",
  "homepage": "https://github.com/gherrink/styleguide/tree/master/packages/rollup#readme",
  "bugs": "https://github.com/gherrink/styleguide/issues",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "prebuild": "rm -rf dist dist",
    "prepare": "if [ ! -d 'dist' ]; then pnpm build; fi",
    "prerelease": "pnpm build",
    "test": "jest"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "@styleguide/html-renderer": "workspace:^0.0.0-alpha.0",
    "rollup": "^4.0.0"
  },
  "dependencies": {
    "@styleguide/core": "workspace:^0.0.0-alpha.0",
    "@styleguide/node": "workspace:^0.0.0-alpha.0",
    "comment-parser": "^1.4.1",
    "marked": "^12.0.2"
  },
  "devDependencies": {
    "@styleguide/core": "workspace:*",
    "@styleguide/html-renderer": "workspace:*",
    "@styleguide/node": "workspace:*"
  }
}
