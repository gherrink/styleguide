{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "html"
  ],
  "cSpell.words": [
    "styleguide"
  ],
  "search.exclude": {
    "**/dist/": true,
  },
  "css.validate": false,
  "[css]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[postcss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
}
