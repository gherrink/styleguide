{
  "name": "styleguide-demos",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "base": "tsx --tsconfig tsconfig.base.json ./base.ts",
    "ts": "tsc --project tsconfig.base.json",
    "rollup": "rollup -c",
    "rollup:watch": "rollup -c -w",
    "serve:base": "tsx --tsconfig tsconfig.base.json ./serve.ts dist/base",
    "serve:rollup": "tsx --tsconfig tsconfig.base.json ./serve.ts dist/rollup"
  },
  "devDependencies": {
    "@highlightjs/cdn-assets": "^11.9.0",
    "@styleguide/core": "workspace:0.0.0-alpha.0",
    "@styleguide/html-renderer": "workspace:0.0.0-alpha.0",
    "@styleguide/node": "workspace:0.0.0-alpha.0",
    "@styleguide/rollup": "workspace:0.0.0-alpha.0",
    "@types/node": "^20.12.7",
    "tsx": "^4.7.2",
    "typescript": "^4.9.5"
  }
}
